# jamroot.jam
project foobar :
  requirements
     #<implicit-dependency>/folly//libs
     #<library>/folly
     #<use>/folly//libs
   <link>shared
   <host-os>linux:<linkflags>"-Wl,--no-as-needed -ldl"
   <address-model>64
   #<architecture>ia64
;

#alias foobar : /Catch2//libs ;
alias foobar : /catch2//libs ;

exe main : main.cpp /folly//libs/<link>shared ;
